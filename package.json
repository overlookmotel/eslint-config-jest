{
  "name": "@overlookmotel/eslint-config-jest",
  "version": "6.0.1",
  "description": "@overlookmotel's ESLint config for tests with jest",
  "main": "index.js",
  "files": [],
  "author": {
    "name": "Overlook Motel"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/overlookmotel/eslint-config-jest.git"
  },
