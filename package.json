    "url": "https://github.com/overlookmotel/eslint-config-jest/issues"
  },
  "dependencies": {},
  "peerDependencies": {
    "eslint": "^8.11.0",
    "eslint-plugin-jest": "^26.1.3"
  },
  "devDependencies": {
    "@overlookmotel/eslint-config": "^10.0.0",
    "@overlookmotel/eslint-config-node": "^4.0.0",
    "eslint": "^8.11.0",
    "eslint-plugin-jest": "^26.1.3",
    "jest": "^29.4.2",
    "jest-extended": "^2.0.0",
    "npm-run-all": "^4.1.5"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
